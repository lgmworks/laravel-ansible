[program:example-worker]
process_name=%(program_name)s_%(process_num)02d
command=php /home/{{ remote_user }}/example.com/artisan queue:work --sleep=3 --tries=3 --timeout 600
autostart=true
autorestart=true
user={{ remote_user }}
numprocs=1
redirect_stderr=true
stdout_logfile={{ code_path }}/worker.log