[program:default]
command=php {{sites['site1'].code_path}}/current/artisan queue:listen --sleep=10 --quiet --timeout=1000 --memory=128 --tries=1

process_name=%(program_name)s_%(process_num)02d
autostart=true
autorestart=true
stopasgroup=true
killasgroup=true
user={{remote_user}}
numprocs=1
stdout_logfile=/home/{{remote_user}}/.{{remote_user}}/site1_default.log
